{
	"compilerOptions": {
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "react-jsx",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"noEmit": true,
		"noUncheckedIndexedAccess": true,
		"paths": {
			"@/*": ["./src/*"]
		},
		"strict": true,
		"target": "ESNext",
		"types": ["vite/client"]
	},
	"include": ["./src"]
}
